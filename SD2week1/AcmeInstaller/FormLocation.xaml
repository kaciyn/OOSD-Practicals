<Window x:Class="AcmeInstaller.FormLocation"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AcmeInstaller"
        mc:Ignorable="d"
        Title="FormLocation" Height="282.146" Width="241.78">
    <Grid>
        <TextBlock x:Name="lblLocations" HorizontalAlignment="Left" TextWrapping="Wrap" VerticalAlignment="Top" Margin="69,23,0,0"><Run Text="Select Your Location"/></TextBlock>
        <ListBox x:Name="lstLocations" Margin="69,49,59,76" SelectionChanged="ListBox_SelectionChanged"/>
        <Button x:Name="btnPrevious" Content="Previous" HorizontalAlignment="Left" Margin="31,196,0,0" VerticalAlignment="Top" Width="75" Click="btnPrevious_btnNext_Click"/>
        <Button x:Name="btnNext" Content="Next" HorizontalAlignment="Left" Margin="132,196,0,0" VerticalAlignment="Top" Width="75" Click="btnPrevious_btnNext_Click"/>

    </Grid>
</Window>
